{
  "id": "http://igt.com/serverAddresses/V1.0",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Schema for serverAddresses",
  "type": "object",
  "properties": {
    "cmd": {
      "type": "string",
      "pattern": "^serverAddresses$"
    },
    "ver": {
      "type": "string"
    },
    "serverAddresses": {
      "type": "array",
      "minItems": 1,
      "maxItems": 16,
      "items": {
        "type": "object"
      }
    }
  },
  "required": ["cmd","serverAddresses"]
}
